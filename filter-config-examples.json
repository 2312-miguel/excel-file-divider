{
  "inputFile": "input.xlsx",
  "sheetName": "Sheet1",
  "outputDir": "filtered_output",
  "filters": [
    {
      "column": "ProductName",
      "operator": "equals",
      "value": "Laptop Pro"
    },
    {
      "column": "Description",
      "operator": "contains",
      "value": "wireless"
    },
    {
      "column": "PlanName",
      "operator": "containsExclude",
      "value": "PLAN FAM",
      "exclude": "mb"
    },
    {
      "column": "ProductType",
      "operator": "containsExclude",
      "value": "Premium",
      "exclude": "Basic"
    },
    {
      "column": "SKU",
      "operator": "startsWith",
      "value": "ELEC"
    },
    {
      "column": "FileExtension",
      "operator": "endsWith",
      "value": ".pdf"
    },
    {
      "column": "Price",
      "operator": "greaterThan",
      "value": 1000
    },
    {
      "column": "Stock",
      "operator": "lessThan",
      "value": 50
    },
    {
      "column": "Category",
      "operator": "equals",
      "value": "Electronics"
    },
    {
      "column": "Status",
      "operator": "contains",
      "value": "active"
    },
    {
      "column": "OrderNumber",
      "operator": "startsWith",
      "value": "ORD-2024"
    },
    {
      "column": "Email",
      "operator": "endsWith",
      "value": "@company.com"
    },
    {
      "column": "Quantity",
      "operator": "greaterThan",
      "value": 0
    },
    {
      "column": "Discount",
      "operator": "lessThan",
      "value": 30
    }
  ]
} 